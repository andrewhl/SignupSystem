<header>
  <nav class="round">
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <% if signed_in? %>
		<li><%= link_to "Users", users_path %></li>
        <li><%= link_to "Profile", current_user %></li>
		<li><%= link_to "Settings", edit_user_path(current_user) %></li>
      <% end %>
	  <% if (signed_in?) and (current_user.admin?) %>
		<li><%= link_to "ASP", after_school_programs_path %></li>
	  <% end %>
      <% if signed_in? %>
        <li><%= link_to "Sign out", signout_path, method: :delete %></li>
      <% else %>
        <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
  <nav class="round sub">
  	<li><%= link_to "View Programs", after_school_programs_path %></li>
	<li><%= link_to "Create Programs", new_after_school_program_path %></li>
	<li><%= link_to "Edit Programs", after_school_programs_edit_all_path %></li>
  </nav>
</header>
